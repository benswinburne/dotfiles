{
  "explorer.sources": [
    {
      "name": "buffer",
      "expand": false
    },
    {
      "name": "file",
      "expand": true
    }
  ],
  "explorer.keyMappings": {
    "<cr>": [
      "expanded?",
      "collapse",
      "expand"
    ],
    "m": [
      "actionMenu"
    ],
    "<C-o>": [
      "cd"
    ]
  },
  "git.enableGlobalStatus": false,
  "explorer.position": "right",
  "explorer.autoCollapseChildren": false,
  "explorer.file.tabCD": false,
  "explorer.previewAction.onHover": false,
  "explorer.width": 40,
  "explorer.icon.enableNerdfont": true,
  "explorer.file.showHiddenFiles": true,
  "typescript.suggestionActions.enabled": false,
  "typescript.suggest.enabled": false,
  "typescript.validate.enable": false,
  "typescript.format.enabled": false,
  "javascript.suggest.enabled": false,
  "javascript.validate.enable": false,
  "diagnostic.displayByAle": true,
  "snippets.loadFromExtensions": true,
  "snippets.extends": {
    "javascriptreact": [
      "javascript"
    ],
    "typescriptreact": [
      "javascript"
    ],
    "typescript": [
      "javascript"
    ]
  },
  "_coc.preferences.hoverTarget": "float",
  "suggest.timeout": 500,
  "suggest.echodocSupport": true,
  "suggest.noselect": false,
  "suggest.maxCompleteItemCount": 30,
  "languageserver": {
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": [
        "dockerfile"
      ],
      "args": [
        "--stdio"
      ]
    }
  },
  "tsserver.formatOnType": false,
  "tsserver.disableAutomaticTypeAcquisition": true,
  "tsserver.watchOptions": {
    "watchFile": "useFsEvents",
    "fallbackPolling": "dynamicPriorityPolling"
  },
  "intelephense.licenceKey": "~/Dropbox/.private/intelephense.txt"
}
